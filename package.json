{
  "name": "password-manager",
  "productName": "PasswordManager",
  "description": "password-manager",
  "version": "0.1.0",
  "license": "MIT",
  "author": {
    "name": "MatsudaYoshio"
  },
  "repository": {
    "url": "https://github.com/MatsudaYoshio/password-manager.git"
  },
  "main": "dist/main.js",
  "scripts": {
    "prepare": "husky",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,json,css,md}\"",
    "lint-and-format": "npm run format && npm run lint:fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "dev": "rimraf dist && run-p dev:webpack dev:electron copy-assets copy-credentials",
    "build": "rimraf dist && cross-env NODE_ENV=\"production\" webpack --progress && npm run copy-assets && npm run copy-credentials && npm run update-package-json",
    "copy-assets": "copyfiles -u 3 src/electron/assets/Aegis.ico dist/assets/",
    "copy-credentials": "copyfiles -u 2 src/credentials/* dist/credentials/",
    "update-package-json": "node scripts/update-package-json.js",
    "dev:webpack": "cross-env NODE_ENV=\"development\" webpack --progress",
    "dev:electron": "wait-on dist/main.js dist/index.html && electronmon .",
    "package": "electron-packager ./dist PasswordManager --icon=./dist/assets/Aegis.ico --overwrite --out=release",
    "release": "npm run build && npm run package"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{js,json,css,md}": [
      "prettier --write"
    ]
  },
  "electronmon": {
    "patterns": [
      "dist/**/*"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@mui/system": "^7.1.1",
    "@mui/x-tree-view": "^8.5.2",
    "@reduxjs/toolkit": "^2.0.1",
    "class-transformer": "^0.5.1",
    "electron-store": "^8.2.0",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "react-resizable-panels": "^3.0.3",
    "redux": "^5.0.1",
    "uuidv7": "^1.0.2"
  },
  "devDependencies": {
    "@electron/packager": "^19.0.1",
    "@eslint/js": "^9.34.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/electron": "^1.6.12",
    "@types/jest": "^29.5.12",
    "@types/jsonfile": "^6.1.4",
    "@types/node": "^24.3.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.2",
    "electron": "^39.2.7",
    "electronmon": "^2.0.2",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.1.2",
    "jest-fixed-jsdom": "^0.0.10",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^16.1.2",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "wait-on": "^8.0.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^6.0.1"
  },
  "volta": {
    "node": "20.19.0"
  }
}
