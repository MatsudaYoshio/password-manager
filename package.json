{
  "name": "password-manager",
  "productName": "PasswordManager",
  "description": "password-manager",
  "version": "0.1.0",
  "license": "MIT",
  "author": {
    "name": "MatsudaYoshio"
  },
  "repository": {
    "url": "https://github.com/MatsudaYoshio/password-manager.git"
  },
  "main": "dist/main.js",
  "scripts": {
    "dev": "rimraf dist && run-p dev:webpack dev:electron",
    "build": "rimraf dist && cross-env NODE_ENV=\"production\" webpack --progress",
    "dev:webpack": "cross-env NODE_ENV=\"development\" webpack --progress",
    "dev:electron": "wait-on dist/main.js dist/index.html && electronmon .",
    "package": "electron-packager . PasswordManager --icon=./src/electron/assets/key.ico --overwrite --out=release"
  },
  "electronmon": {
    "patterns": [
      "dist/**/*"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^7.1.1",
    "@mui/lab": "^7.0.0-beta.13",
    "@mui/material": "^7.1.1",
    "@mui/system": "^7.1.1",
    "@mui/x-tree-view": "^8.5.2",
    "@react-spring/web": "^9.7.3",
    "@reduxjs/toolkit": "^2.0.1",
    "class-transformer": "^0.5.1",
    "electron-store": "^8.2.0",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "react-resizable-panels": "^2.1.7",
    "redux": "^5.0.1",
    "uuidv7": "^1.0.2"
  },
  "devDependencies": {
    "@electron/packager": "^18.3.6",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/electron": "^1.6.12",
    "@types/jsonfile": "^6.1.4",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "electron": "^35.0.0",
    "electronmon": "^2.0.2",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "wait-on": "^7.0.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "volta": {
    "node": "20.19.0"
  }
}
